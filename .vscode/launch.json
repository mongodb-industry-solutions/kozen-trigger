  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "üõ†Ô∏è JavaScript",
      "runtimeExecutable": "npx",
      "console": "integratedTerminal",
      "runtimeArgs": [
        "kozen"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "KOZEN_CONFIG": "kozen.js.json",
        "KOZEN_LOG_LEVEL": "INFO",
        "KOZEN_LOG_TYPE": "object"
      },
    },
    {
      "type": "node",
      "request": "launch",
      "name": "üõ†Ô∏è TypeScript",
      "runtimeExecutable": "ts-node",
      "console": "integratedTerminal",
      "runtimeArgs": [
        "node_modules/@mongodb-solution-assurance/kozen/dist/bin/kozen.js"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "KOZEN_CONFIG": "kozen.ts.json",
        "KOZEN_LOG_LEVEL": "INFO",
        "KOZEN_LOG_TYPE": "object"
      },
    },
    {
      "type": "node",
      "request": "launch",
      "name": "üìÅ Debug Current File",
      "program": "${file}",
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**"
      ],
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "cwd": "${workspaceFolder}"
    }
  ],
  "inputs": []
}